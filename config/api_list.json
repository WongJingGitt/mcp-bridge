  {
    "name": "deepseek",
    "hostname": "chat.deepseek.com",
    "label": "DeepSeek官网",
    "api": ["/api/v0/chat/completion", "/api/v0/chat/edit_message"],
    "promptPath": "prompt",
    "isJsonString": false,
    "enabled": true,
    "defaultAlwaysInject": true,
    "response": {
      "type": "sse",
      "format": "data: {json}",
      "contentPaths": ["v"],
      "filterRules": {
        "pathField": "p",
        "excludePatterns": ["fragments/0/content"]
      }
    },
    "uiParsing": {
      "enabled": true,
      "priority": "ui",
      "messageContainer": ".ds-message",
      "messageIndex": -1,
      "contentSelector": ".ds-markdown"
    },
    "input": {
      "selector": "textarea, [contenteditable='true']",
      "submitKey": "Enter",
      "submitModifiers": [],
      "submitDelay": 1600
    }
  },
