      "contentSelector": ".hyc-common-markdown",
      "senderSelector": ".hyc-content-text"
    },
    "input": {
      "selector": "textarea, [contenteditable='true']",
      "submitKey": "Enter",
      "submitModifiers": [],
      "submitDelay": 2000
    },
    "newConversationFlag": {
      "from": "requestBody",
      "path": "chatModelExtInfo",
      "checkExists": true,
      "checkValue": null,
      "valueType": null
    }
  },
